<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trabalho_lancamentos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.UserApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Trabalho_Lancamentos">
        <activity
            android:name=".ui.activies.ListagemActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activies.EsqueciSenhaActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activies.CadastroMainActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activies.DashboardActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activies.NovoLancamentoActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activies.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>